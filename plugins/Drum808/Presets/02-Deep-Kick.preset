<?xml version="1.0" encoding="UTF-8"?>
<preset name="Deep Kick">
  <!-- Kick Drum - Deep, punchy, low-tuned kick for bass music -->
  <param id="kick_level" value="90.0"/>
  <param id="kick_tone" value="30.0"/>
  <param id="kick_decay" value="600.0"/>
  <param id="kick_tuning" value="-5.0"/>

  <!-- Low Tom - Subdued -->
  <param id="lowtom_level" value="60.0"/>
  <param id="lowtom_tone" value="40.0"/>
  <param id="lowtom_decay" value="250.0"/>
  <param id="lowtom_tuning" value="-3.0"/>

  <!-- Mid Tom - Subdued -->
  <param id="midtom_level" value="60.0"/>
  <param id="midtom_tone" value="40.0"/>
  <param id="midtom_decay" value="200.0"/>
  <param id="midtom_tuning" value="3.0"/>

  <!-- Clap - Crisp -->
  <param id="clap_level" value="75.0"/>
  <param id="clap_tone" value="65.0"/>
  <param id="clap_snap" value="70.0"/>
  <param id="clap_tuning" value="0.0"/>

  <!-- Closed Hi-Hat - Tight -->
  <param id="closedhat_level" value="70.0"/>
  <param id="closedhat_tone" value="55.0"/>
  <param id="closedhat_decay" value="50.0"/>
  <param id="closedhat_tuning" value="0.0"/>

  <!-- Open Hi-Hat - Short -->
  <param id="openhat_level" value="65.0"/>
  <param id="openhat_tone" value="55.0"/>
  <param id="openhat_decay" value="300.0"/>
  <param id="openhat_tuning" value="0.0"/>
</preset>
