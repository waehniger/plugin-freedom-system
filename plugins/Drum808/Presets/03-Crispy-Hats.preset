<?xml version="1.0" encoding="UTF-8"?>
<preset name="Crispy Hats">
  <!-- Kick Drum - Balanced -->
  <param id="kick_level" value="80.0"/>
  <param id="kick_tone" value="50.0"/>
  <param id="kick_decay" value="350.0"/>
  <param id="kick_tuning" value="0.0"/>

  <!-- Low Tom - Subdued -->
  <param id="lowtom_level" value="65.0"/>
  <param id="lowtom_tone" value="45.0"/>
  <param id="lowtom_decay" value="280.0"/>
  <param id="lowtom_tuning" value="0.0"/>

  <!-- Mid Tom - Subdued -->
  <param id="midtom_level" value="65.0"/>
  <param id="midtom_tone" value="45.0"/>
  <param id="midtom_decay" value="230.0"/>
  <param id="midtom_tuning" value="5.0"/>

  <!-- Clap - Subdued -->
  <param id="clap_level" value="68.0"/>
  <param id="clap_tone" value="48.0"/>
  <param id="clap_snap" value="58.0"/>
  <param id="clap_tuning" value="0.0"/>

  <!-- Closed Hi-Hat - Bright, prominent -->
  <param id="closedhat_level" value="80.0"/>
  <param id="closedhat_tone" value="80.0"/>
  <param id="closedhat_decay" value="60.0"/>
  <param id="closedhat_tuning" value="2.0"/>

  <!-- Open Hi-Hat - Bright, long decay -->
  <param id="openhat_level" value="75.0"/>
  <param id="openhat_tone" value="80.0"/>
  <param id="openhat_decay" value="700.0"/>
  <param id="openhat_tuning" value="2.0"/>
</preset>
