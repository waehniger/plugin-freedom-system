<?xml version="1.0" encoding="UTF-8"?>
<preset name="Tight Punch">
  <!-- Kick Drum - Short, punchy, high-tuned -->
  <param id="kick_level" value="85.0"/>
  <param id="kick_tone" value="70.0"/>
  <param id="kick_decay" value="200.0"/>
  <param id="kick_tuning" value="3.0"/>

  <!-- Low Tom - Short attack -->
  <param id="lowtom_level" value="78.0"/>
  <param id="lowtom_tone" value="60.0"/>
  <param id="lowtom_decay" value="150.0"/>
  <param id="lowtom_tuning" value="2.0"/>

  <!-- Mid Tom - Short attack -->
  <param id="midtom_level" value="78.0"/>
  <param id="midtom_tone" value="60.0"/>
  <param id="midtom_decay" value="130.0"/>
  <param id="midtom_tuning" value="7.0"/>

  <!-- Clap - Sharp, snappy -->
  <param id="clap_level" value="80.0"/>
  <param id="clap_tone" value="70.0"/>
  <param id="clap_snap" value="85.0"/>
  <param id="clap_tuning" value="0.0"/>

  <!-- Closed Hi-Hat - Very short -->
  <param id="closedhat_level" value="72.0"/>
  <param id="closedhat_tone" value="65.0"/>
  <param id="closedhat_decay" value="30.0"/>
  <param id="closedhat_tuning" value="0.0"/>

  <!-- Open Hi-Hat - Medium decay -->
  <param id="openhat_level" value="68.0"/>
  <param id="openhat_tone" value="65.0"/>
  <param id="openhat_decay" value="350.0"/>
  <param id="openhat_tuning" value="0.0"/>
</preset>
